<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gospay.ui">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application>
        <activity
            android:name=".payment.PaymentProcessingActivity"
            android:theme="@style/GosWhiteTheme" />
        <activity
            android:name=".card.add.AddCardActivity"
            android:parentActivityName=".payment.PaymentProcessingActivity"
            android:theme="@style/GosWhiteTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".payment.PaymentProcessingActivity" />

        </activity>
    </application>


</manifest>