<resources>

    <!-- Base application theme. -->
    <style name="GosWhiteTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">@color/colorGosPrimary</item>
        <item name="colorPrimaryDark">@color/colorGosPrimaryDark</item>
        <item name="colorAccent">@color/colorAction</item>
    </style>


    <style name="GosDefaultEditText" >
        <item name="android:padding">@dimen/edit_text_padding</item>
        <item name="android:background">@drawable/edit_text_background</item>
        <item name="android:layout_marginTop">@dimen/default_margin</item>
        <item name="android:textSize">14sp</item>
    </style>

</resources>
